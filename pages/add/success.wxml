<view class="page">
    <view class="page__bd">
        <view class="icon-box">
            <icon type="success" class="successIcon" color="#f08519" size="93"></icon>
            <view class="icon-box__ctn">
                <view class="icon-box__title">提交成功！</view>
                <view class="icon-box__desc">请耐心等待审核通过</view>
                <view class="icon-box__desc">{{second}}s后跳转至首页</view>
            </view>
        </view>
        <!--关闭 开始 -->
        <view class="btn-area">
            <button class="loginButton" type='primary' bindtap="closePage">{{buttonText}}</button>
        </view>
        <!--关闭 end -->
    </view>
</view>